<%- include('../partials/header.ejs')%>
<main class="container-fluid">
    <div class="row justify-content-center p-5">
      <div class="col-sm-6 justify-content-center">
        <div class="card p-5 border-info">
          <h4 class="text-center">Delete Category</h4>
          <form action="/trades/deletecat?_method=DELETE" method = 'POST'>
            <label class="mt-3">Category:</label>
            <select name="Category" class="form-control">
                <%category.forEach(cat=>{%>
                <option value="<%=cat.catName%>"><%=cat.catName%></option>   
                <%})%>             
            </select>  <br>                  
            <input class="btn btn-danger mt-3" type="submit" value="Submit">
          </form>
        </div>
      </div>
    </div>
  </main>
<%- include('../partials/footer.ejs')%>